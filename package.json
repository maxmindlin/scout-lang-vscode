{
  "name": "scout-lang-vscode",
  "displayName": "scout-lang-vscode",
  "description": "ScoutLang VSCode extension",
  "repository": "https://github.com/maxmindlin/scout-lang-vscode",
  "publisher": "ScoutLang",
  "icon": "assets/icon.png",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "scout",
        "aliases": [
          "ScoutLang",
          "scout"
        ],
        "extensions": [
          ".sct"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "scout",
        "scopeName": "source.sct",
        "path": "./syntaxes/scout.tmLanguage.json"
      }
    ]
  }
}
